{
  "name": "@automattic/vip",
  "version": "1.2.0-0",
  "description": "The VIP Go Javascript library & CLI",
  "main": "index.js",
  "bin": {
    "vip": "dist/bin/vip.js",
    "vip-app": "dist/bin/vip-app.js",
    "vip-app-list": "dist/bin/vip-app-list.js",
    "vip-sync": "dist/bin/vip-sync.js"
  },
  "scripts": {
    "test": "npm run lint && npm run flow && jest --coverage",
    "prepare": "npm run build",
    "prepack": "npm run prepareConfig:publish",
    "postinstall": "node ./scripts/check-version.js",
    "build": "npm run prepareConfig:local && babel src -d dist",
    "build:watch": "babel src -d dist --watch",
    "flow": "flow",
    "jest": "jest",
    "lint": "eslint -f json . | eslines",
    "prepareConfig:local": "node ./scripts/prepare-config.js local",
    "prepareConfig:publish": "node ./scripts/prepare-config.js publish",
    "publish-please": "publish-please",
    "prepublishOnly": "publish-please guard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/vip.git"
  },
  "keywords": [
    "cli",
    "vip",
    "wordpress"
  ],
  "author": "Automattic",
  "contributors": [
    "Josh Betz <j@joshbetz.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "bugs": {
    "url": "https://github.com/Automattic/vip/issues"
  },
  "homepage": "https://github.com/Automattic/vip#readme",
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-plugin-module-resolver": "~3.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.26.0",
    "babel-preset-flow": "~6.23.0",
    "eslines": "^1.1.0",
    "eslint": "^5.3.0",
    "eslint-config-wpvip": "github:automattic/eslint-config-wpvip#572f21f",
    "flow-bin": "~0.63.1",
    "jest": "~22.1.1",
    "mock-local-storage": "~1.0.5",
    "nock": "~9.3.2",
    "publish-please": "~3.1.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "~1.1.7",
    "apollo-client": "~2.2.3",
    "apollo-link-http": "~1.3.3",
    "args": "~5.0.0",
    "chalk": "~2.4.1",
    "cli-table": "github:automattic/cli-table#b9ebd75",
    "configstore": "^3.1.2",
    "debug": "~3.1.0",
    "graphql": "~0.12.0",
    "graphql-tag": "~2.7.3",
    "ini": "~1.3.5",
    "inquirer": "~5.1.0",
    "isomorphic-fetch": "~2.2.1",
    "json2csv": "~3.11.5",
    "jwt-decode": "~2.2.0",
    "opn": "~5.2.0",
    "semver": "~5.5.0",
    "single-line-log": "~1.1.2",
    "update-notifier": "~2.3.0",
    "uuid": "~3.2.1"
  },
  "eslintConfig": {
    "extends": [
      "wpvip"
    ]
  },
  "optionalDependencies": {
    "keytar": "^4.1.0"
  }
}
