{
  "name": "vip",
  "version": "1.0.0",
  "description": "The VIP Go Javascript library & CLI",
  "main": "index.js",
  "bin": {
    "vip": "bin/vip.js"
  },
  "scripts": {
    "test": "eslint index.js lib bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/vip.git"
  },
  "keywords": [
    "cli",
    "vip",
    "wordpess"
  ],
  "author": "Automattic",
  "contributors": [
    "Josh Betz <j@joshbetz.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=7.10"
  },
  "bugs": {
    "url": "https://github.com/Automattic/vip/issues"
  },
  "homepage": "https://github.com/Automattic/vip#readme",
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "eslint": "^4.14.0",
    "eslint-config-wpcalypso": "0.8.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-wpcalypso": "3.4.0"
  },
  "dependencies": {
    "commander": "^2.12.2",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "keytar": "^4.1.0",
    "promptly": "^2.2.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "plugin:flowtype/recommended",
      "wpcalypso"
    ],
    "plugins": [
      "wpcalypso",
      "flowtype",
      "json"
    ],
    "parserOptions": {
      "sourceType": "module"
    },
    "globals": {
      "describe": true,
      "it": true,
      "expect": true,
      "beforeEach": true,
      "afterEach": true,
      "beforeAll": true,
      "afterAll": true
    },
    "rules": {
      "camelcase": [
        "warn"
      ],
      "no-console": [
        "warn"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "flowtype/no-types-missing-file-annotation": [
        0
      ],
      "no-process-exit": [
        0
      ]
    }
  }
}
