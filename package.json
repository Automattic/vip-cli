{
  "name": "@automattic/vip",
  "version": "2.0.0-dev1",
  "description": "The VIP Javascript library & CLI",
  "main": "index.js",
  "bin": {
    "vip": "dist/bin/vip.js",
    "vip-app": "dist/bin/vip-app.js",
    "vip-app-list": "dist/bin/vip-app-list.js",
